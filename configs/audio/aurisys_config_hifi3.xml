                 lib_path="/vendor/lib/libaurisysdemo.so"
                 lib64_path="/vendor/lib64/libaurisysdemo.so"
                 param_path=""
                 lib_dump_path="AUTO"
                 adb_cmd_key="MTKDCRFLT">
            <components>
                <!-- for aurisys_scene playback normal -->
