                 lib_dump_path="AUTO"
                 adb_cmd_key="SMARTPATFA">
            <components>
                <!-- for aurisys_scene playback normal -->
                <component aurisys_scenario="AURISYS_SCENARIO_DSP_PLAYBACK_SMARTPA"
                           sample_rate="48000"
